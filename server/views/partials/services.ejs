<!--Code Snippet from https://getbootstrap.com/docs/5.0/components/carousel/-->
<div id="carousel" class="carousel slide min-vh-100" data-bs-ride="carousel">
    <div class="carousel-indicators">
      <button type="button" data-bs-target="#carousel" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
      <button type="button" data-bs-target="#carousel" data-bs-slide-to="1" aria-label="Slide 2"></button>
      <button type="button" data-bs-target="#carousel" data-bs-slide-to="2" aria-label="Slide 3"></button>
    </div>
    <!--Carousel images and content-->
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img src="/images/png/ui-ux.png" class="d-block w-100" alt="...">
        <div class="carousel-caption d-none d-md-block">
          <h5 class="display-3 fw-bold">UI/UX Web Design</h5>
          <p class="lead">Client-focused, creating user and customer experiences designed to have engaging user interfaces UI and deliver tangible business results.</p>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/png/mean.png" class="d-block w-100" alt="...">
        <div class="carousel-caption d-none d-md-block">
          <h5 class="display-3 fw-bold">MEAN Fullstack Development</h5>
          <p class="lead">Mean stack development is a full-stack development platform for building highly scalable web and mobile applications.</p>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/png/machinelearning.png" class="d-block w-100" alt="...">
        <div class="carousel-caption d-none d-md-block">
          <h5 class="display-3 fw-bold">Machine Learning Model Training</h5>
          <p class="lead">Improve your business with my Machine learning model training services, I will clean your data, select a model a train it to provide you the best insights and business advantage..</p>
        </div>
      </div>
    </div>
    <!--Carousel controllers-->
    <button class="carousel-control-prev" type="button" data-bs-target="#carousel" data-bs-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carousel" data-bs-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>